{
  "name": "@quckapp/api-client",
  "version": "1.0.0",
  "description": "TypeScript API client for QuckApp",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "generate": "npx openapi-typescript-codegen --input ../../docs/api/openapi-bundled.yaml --output src --client axios --useOptions --useUnionTypes",
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "openapi-typescript-codegen": "^0.27.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "typescript": ">=4.7"
  },
  "keywords": [
    "quckapp",
    "api",
    "client",
    "typescript",
    "openapi"
  ],
  "author": "QuckApp Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quckapp/quckapp.git",
    "directory": "packages/api-client"
  },
  "engines": {
    "node": ">=18"
  }
}
